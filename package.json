{
  "name": "co-tags",
  "version": "0.0.8",
  "description": "Angular 2 tags component",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "build": "npm run tsc",
    "tsc": "tsc -p src",
    "watch": "tsc -p src -w",
    "prepublish": "npm run build"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run build"
    }
  },
  "author": "Carl-Ola Boketoft",
  "license": "ISC",
  "devDependencies": {
    "angular2": "^2.0.0-alpha.46",
    "es6-module-loader": "^0.17.8",
    "express": "^4.13.3",
    "ghooks": "^1.0.0",
    "jasmine": "^2.3.2",
    "jasmine-core": "^2.3.4",
    "jspm": "^0.16.14",
    "karma": "^0.13.15",
    "karma-chrome-launcher": "^0.2.1",
    "karma-jasmine": "^0.3.6",
    "karma-jspm": "^2.0.2",
    "karma-phantomjs-launcher": "^0.2.1",
    "phantomjs": "^1.9.18",
    "systemjs": "^0.19.5",
    "typescript": "^1.6.2"
  },
  "jspm": {
    "configFile": "jspm.config.js",
    "dependencies": {
      "angular2": "npm:angular2@^2.0.0-alpha.46",
      "reflect-metadata": "npm:reflect-metadata@^0.1.2",
      "zone.js": "npm:zone.js@^0.5.8"
    },
    "devDependencies": {
      "typescript": "npm:typescript@^1.6.2"
    }
  }
}
